extends layout

block content

  nav(class="navbar navbar-toggleable-md navbar-light bg-faded first_menu")
    h1= title
    //button(class="navbar-toggler navbar-toggler-right", type="button", data-toggle="collapse", data-target="#navbarText", aria-controls="navbarText", aria-expanded="false", aria-label="Toggle navigation")
    //span(class="navbar-toggler-icon")
    div(class="collapse navbar-collapse", id="navig_public")
      ul(class="projects navbar-nav mr-auto")
        each p in projects
          li(class="link nav-item")
            a(class="nav-link button", href=p.url)= p.title
        else
          li Projects are not existing!

  nav(class="navbar navbar-toggleable-md navbar-light bg-faded")
    div(class="collapse navbar-collapse", id="navig_private")
      ul(class="projects navbar-nav mr-auto")
        li(class="link nav-item logo")
          a(class="nav-link button", href="https://promagen.com")
            img(src="/images/promagen32.png", title="PROMAGEN - PROject MAnager & GENerator")
        li(class="link nav-item logo-text")
          a(class="nav-link button", href="https://promagen.com")="PROMAGEN"
        each p in projects_private
          li(class="link nav-item")
            a(class="nav-link button", href=p.url)= p.title

  div(class='scripts')
  div(class='logs')
    ul
  div(id='containerworldTime')
    iframe(src='http://localhost/', width="992", height="500", scrolling="yes")

  pre(class='message')
